{
  "word_wrap": true,
  "ignored_packages": [
  ],
  "theme": "auto",
  "light_theme": "ayu-light.sublime-theme",
  "dark_theme": "ayu-mirage.sublime-theme",
  "color_scheme": "auto",
  "light_color_scheme": "Packages/ayu/ayu-light.sublime-color-scheme",
  "dark_color_scheme": "Packages/ayu/ayu-mirage.sublime-color-scheme",
  "save_on_focus_lost": true,
  "tab_size": 2,
  "translate_tabs_to_spaces": true,
  "font_size": 13,
  "scroll_past_end": 0.5,
  "find_selected_text": true,
  "vintage_start_in_command_mode": true,
  "vintage_use_clipboard": false,
  "relative_line_numbers": true,
  "show_rel_path": true,
  "show_definitions": false,
  "auto_complete": false,
  "auto_match_enabled": false,
  "auto_close_tags": false,
  "highlight_line_number": false,
  "font_face": "Fira Code",
  "folder_exclude_patterns": [".svn", ".git", ".hg", "CVS", ".Trash", ".Trash-*", "target"],
  "file_exclude_patterns": ["*.pyc", "*.pyo", "*.exe", "*.dll", "*.obj","*.o", "*.a", "*.lib", "*.so", "*.dylib", "*.ncb", "*.sdf", "*.suo", "*.pdb", "*.idb", ".DS_Store", ".directory", "desktop.ini", "*.class", "*.psd", "*.db", "*.sublime-workspace", "*.tasty"], 
  "index_files": true,
}
